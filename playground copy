from datetime import datetime

tide_range = range(-3, 7)
tide_time = 8

count = 0
for tide in tide_range:
    if tide_time == tide:
      count +=1
print(count)  


my_time = datetime.strptime("2025-03-21T21:00:00+00:00", "%Y-%m-%dT%H:%M:%S%z")

print(my_time.strftime("%d.%H.%M"))

midsummer = datetime(2021, 6, 26, 14, 59, 3)

